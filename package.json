{
  "name": "angularjs-typescript-memory-kata",
  "version": "0.0.1",
  "scripts": {
    "postinstall": "jspm install",
    "start": "live-server"
  },
  "jspm": {
    "name": "app",
    "main": "app.js",
    "directories": {
      "lib": "src"
    },
    "devDependencies": {
      "os": "github:jspm/nodelibs-os@^0.2.0-alpha",
      "plugin-typescript": "github:frankwallis/plugin-typescript@^4.0.16"
    },
    "overrides": {
      "npm:typescript@1.8.10": {
        "browser": {},
        "map": {
          "buffer": "@empty",
          "child_process": "@empty",
          "fs": "@empty",
          "path": "@empty",
          "process": "@empty",
          "readline": "@empty"
        }
      }
    }
  },
  "devDependencies": {
    "live-server": "^1.0.0"
  }
}
